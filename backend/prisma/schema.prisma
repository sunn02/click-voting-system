
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Movie {
  id        Int     @id @default(autoincrement())
  title     String
  year      Int 
  poster    String
  genre     String
  imdbID    String
  vote      Int @default(0)     
}